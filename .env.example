# OpenAI key used by the routing orchestrator
OPENAI_API_KEY=

# Optional but recommended for Seattle Open Data rate limits
SOCRATA_APP_TOKEN=

# Local path where NFL Big Data Bowl CSV files are stored
NFL_DATA_DIR=./backend/data/nfl_csvs

# Async SQLite URL for the FastAPI backend
DATABASE_URL=sqlite+aiosqlite:///./backend/safetransit.db

# Comma-separated frontend origins allowed by backend CORS
BACKEND_CORS_ORIGINS=http://localhost:5173
